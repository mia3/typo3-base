{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "composer",
      "url": "http://packages.mia3.com"
    },{
      "type": "vcs",
      "url": "https://github.com/NamelessCoder/flux.git"
    }
  ],
  "require": {
    "typo3/cms": "^8.7",
    "fluidtypo3/flux": "^8.2",
    "fluidtypo3/fluidcontent": "^5.2",
    "fluidtypo3/fluidpages": "^4.1",
    "fluidtypo3/vhs": "^4.2",
    "dmitryd/typo3-realurl": "^2.1",
    "helhum/typo3-console": "^4.4"
  },
  "config": {
    "secure-http": false
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Mia3\\Template\\": "web/typo3conf/ext/template/Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": "0"
    }
  }
}
