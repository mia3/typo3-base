config {
  # The <base> tag in the header of the document which is used by RealURL
  absRefPrefix = /

  # Search. This should be disabled if you are not using Indexed search!
  index_enable = 1
  index_externals = 1

  # Template Extension Path
  template_path = /typo3conf/ext/template

  title = Template
}

page.meta.viewport = width=device-width, initial-scale=1, shrink-to-fit=no
