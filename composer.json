{
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*"
    }
  ],
  "require": {
    "ext-json": "*",
    "fluidtypo3/flux": "^9.4",
    "fluidtypo3/vhs": "^6.0",
    "georgringer/news": "^8.6",
    "helhum/typo3-console": "^6.6",
    "mask/mask": "^7.0",
    "mia3/template": "@dev"
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "typo3-cms-scripts": [
      "typo3cms install:fixfolderstructure",
      "typo3cms install:generatepackagestates"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  },
  "require-dev": {
    "symfony/var-dumper": "^5.0"
  }
}
